{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "build": "nuxt build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "logs:cleanup": "tsx scripts/cleanup-logs.ts",
    "logs:view": "tail -f logs/access.log",
    "logs:view-errors": "tail -f logs/error.log"
  },
  "dependencies": {
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/supabase": "^1.6.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.2",
    "@tiptap/extension-code-block-lowlight": "^2.26.1",
    "@tiptap/extension-youtube": "^2.26.1",
    "@vueuse/core": "^13.6.0",
    "@vueuse/nuxt": "^13.6.0",
    "bcryptjs": "^3.0.2",
    "dompurify": "^3.2.6",
    "highlight.js": "^11.11.1",
    "lightgallery": "^2.9.0-beta.1",
    "lowlight": "^2.9.0",
    "node-fetch": "^2.7.0",
    "nuxt": "^4.0.1",
    "nuxt-tiptap-editor": "^2.3.1",
    "pinia": "^3.0.3",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "rotating-file-stream": "^3.2.6",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.60",
    "@nuxt/test-utils": "^3.19.2",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.54.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
